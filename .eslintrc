{
  "extends": "airbnb-base",
  "plugins": ["jest"],
  "env": {
    "jest/globals": true
  },
  "globals": {
    "describe": true,
    "it": true,
    "true": true
  },
  "rules": {
    "no-underscore-dangle": 0,
    "comma-dangle": ["error", "never"],
    "max-lines-per-function": ["warn", {
      "max": 50
    }],
    "linebreak-style": 0
  }
}
